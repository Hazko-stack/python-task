<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_Revenue_Simple"
                  targetNamespace="http://bpmn.io/schema/bpmn">

  <bpmn:collaboration id="Collaboration_Revenue">
    <bpmn:participant id="Participant_BeeCycle_Revenue" name="BeeCycle (Revenue Cycle)" processRef="Process_Revenue" />
    <bpmn:participant id="Participant_Pelanggan" name="Pelanggan" processRef="Process_Pelanggan" />

    <bpmn:messageFlow id="Flow_PO_to_Sales" name="Kirim PO" sourceRef="Task_Pelanggan_KirimPO" targetRef="StartEvent_PO_Diterima" />
    <bpmn:messageFlow id="Flow_Barang_to_Pelanggan" name="Kirim Barang &amp; Notice" sourceRef="Task_Pengiriman_Kirim" targetRef="Event_Pelanggan_TerimaBarang" />
    <bpmn:messageFlow id="Flow_Invoice_to_Pelanggan" name="Kirim Invoice" sourceRef="Task_Keuangan_KirimInvoice" targetRef="Event_Pelanggan_TerimaInvoice" />
    <bpmn:messageFlow id="Flow_Remittance_to_Keuangan" name="Kirim Bukti Bayar" sourceRef="Task_Pelanggan_KirimRemittance" targetRef="Event_Keuangan_TerimaRemittance" />
    <bpmn:messageFlow id="Flow_Kuitansi_to_Pelanggan" name="Kirim Kuitansi" sourceRef="Task_Keuangan_KirimKuitansi" targetRef="Event_Pelanggan_TerimaKuitansi" />
  </bpmn:collaboration>

  <bpmn:process id="Process_Revenue" isExecutable="false">
    <bpmn:laneSet id="LaneSet_Revenue">
      <bpmn:lane id="Lane_Penjualan" name="Bagian Penjualan">
        <bpmn:flowNodeRef>StartEvent_PO_Diterima</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Penjualan_BuatSO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Penjualan_CetakSO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Penjualan_Split</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Penjualan_Arsip</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Penjualan_Arsip</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Gudang" name="Bagian Gudang">
        <bpmn:flowNodeRef>Task_Gudang_ProsesSO</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Gudang_AmbilKemas</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Gudang_BuatNotice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Gudang_Split</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Gudang_Arsip</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Gudang_Arsip</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Pengiriman" name="Bagian Pengiriman">
        <bpmn:flowNodeRef>Task_Pengiriman_TerimaNotice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Pengiriman_Kirim</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Pengiriman_Kembali</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Pengiriman_Split</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Pengiriman_Arsip</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Pengiriman_Arsip</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Keuangan" name="Bagian Keuangan">
        <bpmn:flowNodeRef>Gateway_Keuangan_Join</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_Cocokkan</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_BuatInvoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Keuangan_Split_Invoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_KirimInvoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_ArsipInvoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_Keuangan_TerimaRemittance</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_CocokkanRemittance</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Keuangan_Decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_HubungiPelanggan</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_BuatKuitansi</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Keuangan_Split_Kuitansi</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_KirimKuitansi</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Keuangan_ArsipKuitansi</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Lunas</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <bpmn:dataStoreReference id="DataStore_SO" name="Sales Order" />
    <bpmn:dataStoreReference id="DataStore_PickingList" name="Picking List" />
    <bpmn:dataStoreReference id="DataStore_PackingList" name="Packing List" />
    <bpmn:dataStoreReference id="DataStore_ShippingNotice" name="Shipping Notice" />
    <bpmn:dataStoreReference id="DataStore_Invoice" name="Invoice" />
    <bpmn:dataStoreReference id="DataStore_Kuitansi" name="Kuitansi" />

    <bpmn:startEvent id="StartEvent_PO_Diterima" name="PO Diterima">
      <bpmn:messageEventDefinition />
    </bpmn:startEvent>
    <bpmn:userTask id="Task_Penjualan_BuatSO" name="Input PO, Buat Sales Order (SO)" />
    <bpmn:userTask id="Task_Penjualan_CetakSO" name="Cetak SO (3 Rangkap)" />
    <bpmn:parallelGateway id="Gateway_Penjualan_Split" />
    <bpmn:userTask id="Task_Penjualan_Arsip" name="Arsipkan SO" />
    <bpmn:endEvent id="End_Penjualan_Arsip" />
    <bpmn:sequenceFlow id="sf_p1" sourceRef="StartEvent_PO_Diterima" targetRef="Task_Penjualan_BuatSO" />
    <bpmn:sequenceFlow id="sf_p2" sourceRef="Task_Penjualan_BuatSO" targetRef="Task_Penjualan_CetakSO" />
    <bpmn:sequenceFlow id="sf_p3" sourceRef="Task_Penjualan_CetakSO" targetRef="Gateway_Penjualan_Split" />
    <bpmn:sequenceFlow id="sf_p_to_gudang" sourceRef="Gateway_Penjualan_Split" targetRef="Task_Gudang_ProsesSO" />
    <bpmn:sequenceFlow id="sf_p_to_keuangan" sourceRef="Gateway_Penjualan_Split" targetRef="Gateway_Keuangan_Join" />
    <bpmn:sequenceFlow id="sf_p_to_arsip" sourceRef="Gateway_Penjualan_Split" targetRef="Task_Penjualan_Arsip" />
    <bpmn:sequenceFlow id="sf_p_arsip_end" sourceRef="Task_Penjualan_Arsip" targetRef="End_Penjualan_Arsip" />
    <bpmn:association id="assoc_so_datastore" sourceRef="Task_Penjualan_BuatSO" targetRef="DataStore_SO" />

    <bpmn:userTask id="Task_Gudang_ProsesSO" name="Terima SO, Buat &amp; Cetak Picking List" />
    <bpmn:userTask id="Task_Gudang_AmbilKemas" name="Ambil Sepeda &amp; Kemas" />
    <bpmn:userTask id="Task_Gudang_BuatNotice" name="Input Packing, Cetak Slip, Buat &amp; Cetak Shipping Notice (4 R)" />
    <bpmn:parallelGateway id="Gateway_Gudang_Split" />
    <bpmn:userTask id="Task_Gudang_Arsip" name="Arsipkan Notice (1 R)" />
    <bpmn:endEvent id="End_Gudang_Arsip" />
    <bpmn:sequenceFlow id="sf_g1" sourceRef="Task_Gudang_ProsesSO" targetRef="Task_Gudang_AmbilKemas" />
    <bpmn:sequenceFlow id="sf_g2" sourceRef="Task_Gudang_AmbilKemas" targetRef="Task_Gudang_BuatNotice" />
    <bpmn:sequenceFlow id="sf_g3" sourceRef="Task_Gudang_BuatNotice" targetRef="Gateway_Gudang_Split" />
    <bpmn:sequenceFlow id="sf_g_to_pengiriman" sourceRef="Gateway_Gudang_Split" targetRef="Task_Pengiriman_TerimaNotice" />
    <bpmn:sequenceFlow id="sf_g_to_arsip" sourceRef="Gateway_Gudang_Split" targetRef="Task_Gudang_Arsip" />
    <bpmn:sequenceFlow id="sf_g_arsip_end" sourceRef="Task_Gudang_Arsip" targetRef="End_Gudang_Arsip" />
    <bpmn:association id="assoc_picking_datastore" sourceRef="Task_Gudang_ProsesSO" targetRef="DataStore_PickingList" />
    <bpmn:association id="assoc_packing_datastore" sourceRef="Task_Gudang_BuatNotice" targetRef="DataStore_PackingList" />
    <bpmn:association id="assoc_notice_datastore" sourceRef="Task_Gudang_BuatNotice" targetRef="DataStore_ShippingNotice" />

    <bpmn:userTask id="Task_Pengiriman_TerimaNotice" name="Terima Notice (3 R) &amp; Ambil Barang" />
    <bpmn:userTask id="Task_Pengiriman_Kirim" name="Kirim Barang, Dapat TTD, Beri Notice (1 R)" />
    <bpmn:userTask id="Task_Pengiriman_Kembali" name="Kembali ke Kantor" />
    <bpmn:parallelGateway id="Gateway_Pengiriman_Split" />
    <bpmn:userTask id="Task_Pengiriman_Arsip" name="Arsipkan Notice (1 R TTD)" />
    <bpmn:endEvent id="End_Pengiriman_Arsip" />
    <bpmn:sequenceFlow id="sf_pe1" sourceRef="Task_Pengiriman_TerimaNotice" targetRef="Task_Pengiriman_Kirim" />
    <bpmn:sequenceFlow id="sf_pe2" sourceRef="Task_Pengiriman_Kirim" targetRef="Task_Pengiriman_Kembali" />
    <bpmn:sequenceFlow id="sf_pe3" sourceRef="Task_Pengiriman_Kembali" targetRef="Gateway_Pengiriman_Split" />
    <bpmn:sequenceFlow id="sf_pe_to_keuangan" sourceRef="Gateway_Pengiriman_Split" targetRef="Gateway_Keuangan_Join" />
    <bpmn:sequenceFlow id="sf_pe_to_arsip" sourceRef="Gateway_Pengiriman_Split" targetRef="Task_Pengiriman_Arsip" />
    <bpmn:sequenceFlow id="sf_pe_arsip_end" sourceRef="Task_Pengiriman_Arsip" targetRef="End_Pengiriman_Arsip" />
    
    <bpmn:parallelGateway id="Gateway_Keuangan_Join" name="Validasi Dokumen" />
    <bpmn:userTask id="Task_Keuangan_Cocokkan" name="Cocokkan SO &amp; Shipping Notice TTD" />
    <bpmn:userTask id="Task_Keuangan_BuatInvoice" name="Buat &amp; Cetak Invoice (2 R)" />
    <bpmn:parallelGateway id="Gateway_Keuangan_Split_Invoice" />
    <bpmn:userTask id="Task_Keuangan_KirimInvoice" name="Kirim Invoice ke Pelanggan" />
    <bpmn:userTask id="Task_Keuangan_ArsipInvoice" name="Arsipkan Invoice, SO, Notice" />
    <bpmn:intermediateCatchEvent id="Event_Keuangan_TerimaRemittance" name="Terima Bukti Bayar">
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    <bpmn:userTask id="Task_Keuangan_CocokkanRemittance" name="Cocokkan Remittance &amp; Invoice" />
    <bpmn:exclusiveGateway id="Gateway_Keuangan_Decision" name="Pembayaran Sesuai?" />
    <bpmn:userTask id="Task_Keuangan_HubungiPelanggan" name="Hubungi Pelanggan (Kekurangan)" />
    <bpmn:userTask id="Task_Keuangan_BuatKuitansi" name="Buat &amp; Cetak Kuitansi (2 R)" />
    <bpmn:parallelGateway id="Gateway_Keuangan_Split_Kuitansi" />
    <bpmn:userTask id="Task_Keuangan_KirimKuitansi" name="Kirim Kuitansi ke Pelanggan" />
    <bpmn:userTask id="Task_Keuangan_ArsipKuitansi" name="Arsipkan Kuitansi &amp; Remittance" />
    <bpmn:endEvent id="EndEvent_Lunas" name="Lunas" />
    <bpmn:sequenceFlow id="sf_k1" sourceRef="Gateway_Keuangan_Join" targetRef="Task_Keuangan_Cocokkan" />
    <bpmn:sequenceFlow id="sf_k2" sourceRef="Task_Keuangan_Cocokkan" targetRef="Task_Keuangan_BuatInvoice" />
    <bpmn:sequenceFlow id="sf_k3" sourceRef="Task_Keuangan_BuatInvoice" targetRef="Gateway_Keuangan_Split_Invoice" />
    <bpmn:sequenceFlow id="sf_k4" sourceRef="Gateway_Keuangan_Split_Invoice" targetRef="Task_Keuangan_KirimInvoice" />
    <bpmn:sequenceFlow id="sf_k5" sourceRef="Gateway_Keuangan_Split_Invoice" targetRef="Task_Keuangan_ArsipInvoice" />
    <bpmn:sequenceFlow id="sf_k6" sourceRef="Task_Keuangan_ArsipInvoice" targetRef="Event_Keuangan_TerimaRemittance" />
    <bpmn:sequenceFlow id="sf_k7" sourceRef="Event_Keuangan_TerimaRemittance" targetRef="Task_Keuangan_CocokkanRemittance" />
    <bpmn:sequenceFlow id="sf_k8" sourceRef="Task_Keuangan_CocokkanRemittance" targetRef="Gateway_Keuangan_Decision" />
    <bpmn:sequenceFlow id="sf_k_yes" name="Sesuai" sourceRef="Gateway_Keuangan_Decision" targetRef="Task_Keuangan_BuatKuitansi" />
    <bpmn:sequenceFlow id="sf_k_no" name="Tidak Sesuai" sourceRef="Gateway_Keuangan_Decision" targetRef="Task_Keuangan_HubungiPelanggan" />
    <bpmn:sequenceFlow id="sf_k_loop" sourceRef="Task_Keuangan_HubungiPelanggan" targetRef="Event_Keuangan_TerimaRemittance" />
    <bpmn:sequenceFlow id="sf_k9" sourceRef="Task_Keuangan_BuatKuitansi" targetRef="Gateway_Keuangan_Split_Kuitansi" />
    <bpmn:sequenceFlow id="sf_k10" sourceRef="Gateway_Keuangan_Split_Kuitansi" targetRef="Task_Keuangan_KirimKuitansi" />
    <bpmn:sequenceFlow id="sf_k11" sourceRef="Gateway_Keuangan_Split_Kuitansi" targetRef="Task_Keuangan_ArsipKuitansi" />
    <bpmn:sequenceFlow id="sf_k12" sourceRef="Task_Keuangan_ArsipKuitansi" targetRef="EndEvent_Lunas" />
    <bpmn:association id="assoc_invoice_datastore" sourceRef="Task_Keuangan_BuatInvoice" targetRef="DataStore_Invoice" />
    <bpmn:association id="assoc_kuitansi_datastore" sourceRef="Task_Keuangan_BuatKuitansi" targetRef="DataStore_Kuitansi" />

  </bpmn:process>

  <bpmn:process id="Process_Pelanggan" isExecutable="false">
    <bpmn:startEvent id="Start_Pelanggan" name="Mulai Pesan" />
    <bpmn:userTask id="Task_Pelanggan_KirimPO" name="Buat &amp; Kirim PO" />
    <bpmn:intermediateCatchEvent id="Event_Pelanggan_TerimaBarang" name="Terima Barang &amp; TTD Notice">
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="Event_Pelanggan_TerimaInvoice" name="Terima Invoice">
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    <bpmn:userTask id="Task_Pelanggan_KirimRemittance" name="Bayar &amp; Kirim Bukti Bayar" />
    <bpmn:intermediateCatchEvent id="Event_Pelanggan_TerimaKuitansi" name="Terima Kuitansi">
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    <bpmn:endEvent id="End_Pelanggan" name="Pesanan Selesai" />
    <bpmn:sequenceFlow id="sf_c1" sourceRef="Start_Pelanggan" targetRef="Task_Pelanggan_KirimPO" />
    <bpmn:sequenceFlow id="sf_c2" sourceRef="Task_Pelanggan_KirimPO" targetRef="Event_Pelanggan_TerimaBarang" />
    <bpmn:sequenceFlow id="sf_c3" sourceRef="Event_Pelanggan_TerimaBarang" targetRef="Event_Pelanggan_TerimaInvoice" />
    <bpmn:sequenceFlow id="sf_c4" sourceRef="Event_Pelanggan_TerimaInvoice" targetRef="Task_Pelanggan_KirimRemittance" />
    <bpmn:sequenceFlow id="sf_c5" sourceRef="Task_Pelanggan_KirimRemittance" targetRef="Event_Pelanggan_TerimaKuitansi" />
    <bpmn:sequenceFlow id="sf_c6" sourceRef="Event_Pelanggan_TerimaKuitansi" targetRef="End_Pelanggan" />
  </bpmn:process>

